definitions:
  errorResponse:
    type: object
    properties:
      errors:
        type: array
        items:
          type: object
          properties:
            status_code:
              type: string
              description: Código de error
            message:
              type: string
              description: Mensaje de error
  RESPONSE_RetrieveExternalTransfersResponse:
    description: ''
    required:
      - RetrieveExternalTransfersResponse
    type: object
    properties:
      RetrieveExternalTransfersResponse:
        description: 'Objeto que representa al cuerpo de mensaje'
        properties:
          InstructionIdentification:
            type: string
            description: 'Autogenerado de identificación única de transacción'
            example: '5f1a4342e318e8f44cf59b48'   
          Creditor:
            type: object
            description: 'Objeto que representa al creditor quien recibe el dinero'
            properties:
              PartyIdentification:
                type: object
                description: Objeto que almacena información relacionada al creditor
                properties:
                  IdentificationNumber:
                    type: integer
                    description: Número de cuenta receptor
                    example: 45587745
                  Name:
                    type: string
                    description: Nombre del receptor
                    example: Jhon Doe
              Account:
                type: object
                description: 'Objeto que contiene la información de la dirección de la cuenta del creditor'
                properties:
                  CCIIdentification:
                    type: string
                    description: 'Código de Cuenta Interbancario del Cliente Receptor '
                    example: '1234011-0052-89-0001234567856789'
              PhoneAddress:
                type: object
                description: 'Objeto que contiene la información de la dirección del teléfono'
                properties:
                  MobileNumber:
                    type: string
                    description: 'Número de celular del Cliente Receptor'
                    example: '+51998885474'
              BankOperation:
                type: object
                properties:
                  CustomerIndicator:
                    type: string
                    description: Indicador booleano que determina si la operación o consulta se está realizando para el mismo cliente.
                    example: true            
          CreditorAgent:
            description: Objeto que almacena la información del creditor 
            properties:
              Location:
                description: Objeto que contiene los datos de locación del creditor
                properties:
                  LocationCode:
                    type: integer
                    description: Código de la agencia de la entidad del creditor
                    example: 1
              FinancialInstitution:
                description: Objeto que almacena la entidad de la institución financiera del creditor
                properties:
                  Identification:
                    type: string
                    description: Código de la entidad destino
                    example: 1
          Debtor:
            type: object
            description: 'Objeto que representa al debtor quien envia el dinero'
            properties:
              PersonName:
                type: object
                description: 'Objeto que representa al el nombre del cliente originante'
                properties:
                  FullName:
                    type: string
                    description: 'Nombre del Cliente Originante'
                    example: 'Jhon James'
              BankAccounts:
                type: array
                description: 'Arreglo de objetos que contienen la información de las cuentas de ahorro'
                items:
                  type: object
                  properties:
                    AccountIdentification:
                      type: string
                      description: 'Número de identificación de la cuenta de ahorros'
                      example: '12345678901234'
                    CCIIdentification:
                      type: string
                      description: 'Código de Cuenta Interbancario (CCI)'
                      example: '00212345678901234567'
                    ProductIdentification:
                      type: integer
                      description: 'Identificador del producto'
                      example: 2
  REQUEST_RegisterExternalTransfers:
    description: Objeto que contiene el cuerpo del mensaje de entrada
    required:
    - RegisterExternalTransfers
    type: object
    properties:
      RegisterExternalTransfers:
        type: object
        required:
        - RegisterExternalTransfers
        description: Objeto que contiene la información para registrar las transferencias interbancarias
        properties:
          Account:
            type: object
            description: Objeto que administra la información de las cuentas
            properties:
              AccountType:
                type: object
                description: Información del tipo de cuenta
                properties:
                  AccountTypeIdentifier:
                    type: integer
                    description: Código del tipo de Cuenta
                    example: 1
          PersonIdentification:
            type: object
            properties:
              PersonType:
                type: object
                description: Información del tipo de persona
                properties:
                  PersonTypeIdentifier:
                    type: integer
                    description: Código del tipo de persona
                    example: 1
          Product:
            type: object
            description: Objeto que contiene la información de los productos
            properties:
              Module:
                type: object
                description: Objeto que contiene la información del módulo del producto
                properties:
                  ModuleReference:
                    type: integer
                    description: Código del módulo Ahorros / Créditos
                    example: 0
              Channel:
                type: object
                description: Objeto que contiene la información del canal
                properties:
                  ChannelReference:
                    type: integer
                    description: Código del canal de apertura Ventanilla ó Aplicativo
                    example: 101
                  ChannelDescription:
                    type: string
                    description: Descripción del canal de apertura
                    example: Ventanilla
              Productidentification:
                type: object
                description: Objeto que contiene la información de la identificación delproducto
                properties:
                  Identifier:
                    type: integer
                    description: Código del producto de la operación
                    example: 101
          Transaction:
            type: object
            description: Objeto que contiene la información de las transacciones
            properties:
              TransactionTypeIdentifier:
                type: integer
                description: Código de tipo de operación interbancaria
                example: 2
              TransactionTypeName:
                type: string
                description: Nombre o descripción del tipo de operación interbancaria 
              CurrencyDetails:
                type: object
                description: Objeto que contiene la información de la moneda
                properties:
                  CurrencyInternalIdentifier:
                    type: integer
                    description: Identificador interno de la caja
                    example: 1
                  CurrencyCodeIdentifier:
                    type: integer
                    description: Identificador de la moneda según ISO 4217
                    example: 604
                  CurrencyCode:
                    type: string
                    description: Código de la moneda según ISO 4217
                    example: "PEN"
                  CurrencyName:
                    type: string
                    description: Nombre de la moneda en el idioma local 
                    example: "Soles"  
              Channel:
                type: object
                description: Objeto que contiene la información del canal
                properties:
                  ChannelReference:
                    type: integer
                    description: Código del canal de operación Ventanilla ó Aplicativo
                    example: 101
                  ChannelDescription:
                    type: string
                    description: Descripción del canal de operación
                    example: Operación de transferencia
                            
          IdentificationOrContactInformation:
            type: object
            description: Objeto que almacena la modalidad de la consulta
            properties:
              Identification:
                type: integer
                description: Código de consulta (CCI / INTERO.)
                example: 501
              Name:
                type: string
                description: Nombre de la modalidad destino                                            
                example: CCI  
              
          FinancialInstitution:
            type: object
            description: Objeto que identifica la entidad financiera
            properties:
              Identification:
                type: string
                description: Código de la entidad destino
                example: 501
              Name:
                type: string 
                description: Descripción de la entidad destino                
          Commissions:
            type: object
            description: Objeto que contiene la información relacionada a comisiones
            properties:
              InterplazaTransactionIndicator:
                type: boolean
                description: Aplica Inter-Plaza/Plaza
                example: false
              VariableRateIndicator:
                type: boolean
                description: Indicador para determinar si aplica ComisionFija / Variable
                example: false
              MonthlyFreeOperationCount:
                type: integer
                description: Cantidad de operación gratis por mes
                example: 1
              ActiveCommissionIndicator:
                type: boolean
                description: Vigencia de la configuración
                example: false
              Amount:
                type: object
                description: Objeto que contiene los montos de las comisiones
                properties:
                  Value:
                    type: number
                    description: Monto de la comisión
                    example: 2000.5
              VariableAmount:
                type: object
                description:  Objeto que contiene montos variables  
                properties:
                  MinimumValue:
                    type: number
                    description: Monto valor máximo
                    example: 2.55
                  MaximumValue:
                    type: number
                    description: Monto valor mínimo
                    example: 10.25
