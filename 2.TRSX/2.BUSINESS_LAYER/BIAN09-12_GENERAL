swagger: '2.0'
info:
  title: payment-to-information-association-b
  version: 1.0.0
  description: Exponer la api de cálculo de comisiones para transferencias internas y externas para cuenta digital
basePath: /payment-to-information-association-b/v1
paths:
  /external-transfers/retrieve:
    get:
      tags:
      - BQ - ExternalTransfers
      operationId: getExternalTransfers
      description: Expone la consulta de transferencia por recurso. Obtiene la información según cuenta Interbancaria CCI ó número de teléfono
      summary: Expone la consulta de transferencia por recurso. Obtiene la información según cuenta Interbancaria CCI ó número de teléfono
      produces:
        - application/json
      responses:
        '200':
          schema:
            $ref: >-
              #/definitions/RESPONSE_RetrieveExternalTransfersResponse
          description: OK
        '204':
          description: No Content
        '400':
          schema:
            $ref: >-
              #/definitions/errorResponse
          description: Bad Request
        '401':
          schema:
            $ref: >-
              #/definitions/errorResponse
          description: Unauthorized
        '404':
          schema:
            $ref: >-
              #/definitions/errorResponse
          description: Not Found
        '422':
          schema:
            $ref: >-
              #/definitions/errorResponse
          description: Unprocessable Content
        '500':
          schema:
            $ref: >-
              #/definitions/errorResponse
          description: Internal Server Error
        '502':
          schema:
            $ref: >-
              #/definitions/errorResponse
          description: Bad Gateway
        '504':
          schema:
            $ref: >-
              #/definitions/errorResponse
          description: Gateway Timeout
      parameters:
        - name: channelIdentification
          in: header
          description: Indicador del canal
          required: true
          type: string
        - name: messageIdentification
          in: header
          description: Indicador del mensaje
          required: true
          type: string
        - name: typeOfIdentification
          in: query
          description: Determina el ID del tipo de documento.    
          required: true
          type: integer
        - name: identityCardNumber
          in: query
          description: Número asignado por una autoridad nacional a una tarjeta de identidad.
          required: true
          type: string
        - name: identificationOrContactInformation
          in: query
          description: Codigo de consulta (CCI / INTERO.)
          required: true
          type: integer
        - name: identificationOrContactValue
          in: query
          description: Número de identificación de la operación, puede ser un Código de Cuenta Interbancario (CCI) o un número de celular
          required: true
          type: string  
        - name: financialInstitutionIdentification
          in: query
          description: Código de la entidad Destino
          required: true
          type: integer
        - name: currencyInternalIdentifier
          in: query
          description: Código interno del tipo de moneda
          type: integer
