  RESPONSE_RetrieveExternalTransfersFeesResponse:
    description: Objeto que contiene el cuerpo del mensaje de salida
    required:
      - RetrieveExternalTransfersFeesResponse
    type: object
    properties:
      RetrieveExternalTransfersFeesResponse:
        type: object
        description: Objeto que contiene la lista de comisiones para trasnferencia de cuenta digital
        properties:
          Commissions:
            type: array
            description: Objeto que contiene la lista de comisiones
            items:
              type: object
              properties:
                Commission:
                  type: object
                  description: Objeto que contiene las comisiones
                  properties:
                    Account:
                      type: object
                      description: Objeto que administra la información de las cuentas
                      properties:
                        AccountType:
                          type: object
                          description: Información del tipo de cuenta
                          properties:
                            AccountTypeValue:
                              type: string
                              description: Tipo de cuenta
                              example: Individual
                            AccountTypeIdentifier:
                              type: integer
                              description: Código del tipo de Cuenta
                              example: 1
                    PersonIdentification:
                      type: object
                      properties:
                        PersonType:
                          type: object
                          description: Información del tipo de persona
                          properties:
                            PersonTypeValue:
                              type: string
                              description: Tipo de persona
                              example: Natural
                            PersonTypeIdentifier:
                              type: integer
                              description: Código del tipo de persona
                              example: 1
                    Code:
                      type: string
                      description: Código de la comisión
                      example: 1234
                    ActiveCommissionIndicator:
                      type: boolean
                      description: Indicador de Vigencia de la comisión
                      example: true
                    MonthlyFreeOperationCount:
                      type: integer
                      description: Cantidad de operaciones libres por mes
                      example: 5
                    InterplazaTransactionIndicator:
                      type: boolean
                      description: Indicador para determinar si es Interplaza ó plaza
                      example: false
                    VariableRateIndicator:
                      type: boolean
                      description: Indicador para determinar si es tasa Variable ó Fijo
                      example: false
                    Amount:
                      type: object
                      properties:
                        Value:
                          type: number
                          description: Monto comisión
                          example: 7.25
                    VariableAmount:
                      type: object
                      properties:
                        MinimumValue:
                          type: number
                          description: Monto mínimo variable
                          example: 1.25
                        MaximumValue:
                          type: number
                          description: Monto máximo variable
                          example: 7.25
                    Transaction:
                      description: Información de las comisiones
                      type: object
                      properties:
                        Code:
                          type: integer
                          description: Código de concepto de operación
                          example: 567
                        AdditionalInformation:
                          type: integer
                          description: Descripción del concepto de la operación
                          example: Operación de transferencia
                        Channel:
                          description: Objeto que contiene información sobre el canal
                          type: object
                          properties:
                            ChannelReference:
                              type: integer
                              description: Código de canal de operación
                              example: 567
                            ChannelDescription:
                              type: string
                              description: Descripción del canal de operación
                              example: App
                    Product:
                      description: Objeto que contiene la información de los productos
                      type: object
                      properties:
                        Productidentification:
                          type: object
                          description: Objeto que contiene la información de la identificación del producto
                          properties:
                            Identifier:
                              type: integer
                              description: Código del producto de la operación
                              example: 101
                        Channel:
                          type: object
                          description: Objeto que contiene la información del canal
                          properties:
                            ChannelReference:
                              type: integer
                              description: Código del canal de apertura Ventanilla ó Aplicativo
                              example: 101   
                            ChannelDescription:
                              type: string
                              description: Descripción del canal de apertura
                              example: Ventanilla
                    IdentificationOrContactInformation:
                      type: object
                      description: Objeto que almacena la modalidad de la consulta
                      properties:
                        Identification:
                          type: integer
                          description: Código de consulta (CCI / INTERO.)
                          example: 501
                        Name:
                          type: string
                          description: Nombre de la modalidad destino                                            
                          example: CCI
                    CreditorAgent:
                      type: object
                      description: Objeto que identifica al creditor  
                      properties:
                        FinancialInstitutionIdentification:
                          type: object
                          description: Objeto que identifica la entidad financiera del creditor
                          properties:
                            Identification:
                              type: string
                              description: Código de la entidad DESTINO
                              example: 501
                            Name:
                              type: string
                              description: Descripción de la entidad destino
                              example: 501
                    CurrencyDetails:
                      type: object
                      description: Objeto que contiene la información de la moneda
                      properties:
                        CurrencyInternalIdentifier:
                          type: integer
                          description: Identificador interno de la caja
                          example: 1
                        CurrencyCodeIdentifier:
                          type: integer
                          description: Identificador de la moneda según ISO 4217
                          example: 604
                        CurrencyCode:
                          type: string
                          description: Código de la moneda según ISO 4217
                          example: "PEN"
                        CurrencyName:
                          type: string
                          description: Nombre de la moneda en el idioma local 
                          example: "Soles"       
  REQUEST_RetrieveContactSweep:
    type: object
    properties:
      RetrieveContactSweep:
        type: object
        properties:
          DebtorAgent:
            type: object
            properties:
              FinancialInstitution:
                type: object
                properties:
                  Identification:
                    type: string
                    description: Código de la Entidad Originante
                    example: "0881"
          MessageIdentification:
            type: object            
            properties:
              CCEHash:
                example: "09876543-21ab-43de-85cd-9876543210ab"
                description: Identificador de la traza
                type: string 
          Contact:
            type: object
            properties:
              CustomerPhoneNumber:
                type: string
                description: Número de teléfono del Cliente (Valor Dummy) 
                example: "+51999999999"
          ContactList:
            type: array
            description: Directorio de contactos del cliente
            items:
              type: string
            example:
              - "+519521454477"
              - "+519876543210"
              - "+519998887770"
  RESPONSE_RetrieveContactSweepResponse:
    type: object
    properties:
      RetrieveContactSweepResponse:
        type: array
        items:
          type: object
          properties:
            ContactList:
              type: array
              items:
                type: object
                properties:
                  Contact:
                    type: object
                    properties:
                      PhoneNumber:
                        type: string
                        description: Número de celular
                        example: "+519521454477"
                  BankingServiceDetails:
                    type: array
                    description: Lista de directorios y códigos de entidad
                    items:
                      type: object
                      properties:
                        DirectoryCode:
                              type: string
                              description: Código de directorio
                              example: 0901
                        CreditorAgent:
                          type: object
                          properties:
                            FinancialInstitution:
                              type: object
                              properties:
                                Identification:
                                  type: string
                                  description: Código de la Entidad Originante
                                  example: "0555"   
  